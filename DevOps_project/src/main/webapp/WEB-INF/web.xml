<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>DevOps_project</display-name>

	<servlet>
		<servlet-name>UserServlet_1</servlet-name>
		<servlet-class>com.dvops.maven.eclipse.UserServlet</servlet-class> 
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UserServlet_1</servlet-name>
		<url-pattern>/UserServlet/dashboard</url-pattern>
		<url-pattern>/UserServlet/edit</url-pattern>
		<url-pattern>/UserServlet/update</url-pattern>
		<url-pattern>/UserServlet/delete</url-pattern>
		<url-pattern>/UserServlet/review</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ProductServlet</servlet-name>
		<servlet-class>com.dvops.maven.eclipse.ProductServlet</servlet-class>
	</servlet>
  
	<servlet-mapping>
		<servlet-name>ProductServlet</servlet-name>
		<url-pattern>/ProductServlet/listDisplay</url-pattern>
		<url-pattern>/ProductServlet/get_name</url-pattern>
		<url-pattern>/ProductServlet/review</url-pattern>
	</servlet-mapping>

</web-app>